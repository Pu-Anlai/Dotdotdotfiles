xkb_symbols "custom" {

key <COMP> { [ Multi_key ]};
key <PRSC> { [ Multi_key ]};

key <CAPS> {
        type= "CTRL+ALT",
        symbols = [ Escape, Escape, Caps_Lock, Escape, Escape ],
        actions = [ NoAction(), NoAction(), LockMods(modifiers=Lock), NoAction(), NoAction() ]
};

key <AC03>	{
        type = "EIGHT_LEVEL_ALPHABETIC",
        symbols = [ d, D, bracketleft, braceleft, eth, ETH, dead_belowbreve ]
};

key <AC04>	{
        type = "EIGHT_LEVEL_ALPHABETIC",
        symbols = [ f, F, bracketright, braceright, U0294, ordfeminine, dead_belowcircumflex ]
};

};
